  rate_during[j]<-sum(time_count[(lower_limit+2):(lower_limit+1+(upper_limit-lower_limit))])
  rate_post[j]<-sum(time_count[(lower_limit+2+(upper_limit-lower_limit)):(window_length+1)])
  mouse_2_bin[[j]]<-time_count 
  time_count<-0
}

