            colStim = "grey80",
            xlim=c(xlim_ll,xlim_ul), 
            ylim=c(ylim_ll,ylim_ul),
            "l", 
            xlab="Time",
            ylab="Reward Trial", 
            main=paste("Mouse",j))
dev.off()  


